<div id="search_content">  <%=form_tag('/search/', id: 'search_form', method: 'get', remote: true) do  %>   	<div class="spinner-container">  		<div class="loading-left"></div>  	</div>  	<%= text_field_tag 'search', nil, placeholder: 'Search Recipes' %>    <!-- data: { disable_with: "Please wait..." } -->    <%= button_tag('search', id: 'form_submit_button') %>  <% end %></div>